{
  "name": "handsketched",
  "version": "0.0.1",
  "description": "A website for Daniel Fallon that shows off his whimsical style",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "test": "echo \"No test system has been written yet\" && exit 0",
    "clean": "rimraf ./dist",
    "prebuild": "tsc --project ./src/build/tsconfig.json",
    "build": "npm run build:webpack:client",
    "build:webpack:client": "webpack --config ./dist/build/webpack.config.client.js",
    "run:prod": "node ./dist",
    "node": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheElf/HandSketched.git"
  },
  "author": "Daniel Fallon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheElf/HandSketched/issues"
  },
  "homepage": "https://github.com/TheElf/HandSketched#readme",
  "devDependencies": {
    "@types/express": "^4.0.34",
    "@types/node": "^6.0.52",
    "@types/webpack": "^2.0.0",
    "@types/webpack-merge": "0.0.4",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "rimraf": "^2.5.4",
    "source-map-loader": "^0.1.5",
    "typescript": "^2.1.4",
    "webpack": "^2.2.0-rc.1",
    "webpack-merge": "^1.1.2"
  },
  "dependencies": {
    "@types/react": "^0.14.55",
    "@types/react-dom": "^0.14.19",
    "express": "^4.14.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  }
}
